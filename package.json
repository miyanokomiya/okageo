{
  "name": "okageo",
  "version": "1.0.18",
  "description": "parse SVG to polygons",
  "main": "./dist/okageo.js",
  "author": "miyanokomiya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miyanokomiya/okageo.git"
  },
  "keywords": [
    "svg"
  ],
  "types": "./types/index.d.ts",
  "scripts": {
    "lint": "yarn eslint ./src ./test ./types ./*.js --ext .js,.ts && yarn prettier ./*.json ./.*.json",
    "lint:fix": "yarn eslint --fix ./src ./test ./types ./*.js --ext .js,.ts && yarn prettier --write ./*.json ./.*.json",
    "test": "jest --coverage",
    "build": "parcel build src/okageo.ts",
    "doc": "typedoc --out doc",
    "demo": "parcel demo/index.html",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.2.2"
  }
}
